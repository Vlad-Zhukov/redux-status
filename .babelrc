{
  "env": {
    "test": {
      "presets": [
        ["env", {
          "targets": {"node": "current"}
        }],
        "react",
        "stage-0"
      ]
    },
    "development": {
      "presets": [
        ["env", {
          "targets": {"node": 6, "browsers": ["> 1%"]},
          "modules": false
        }],
        "react",
        "stage-0"
      ],
      "plugins": ["external-helpers"]
    }
  }
}
